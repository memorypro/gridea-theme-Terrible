<div class="pagination-container">
  <% if (pagination.prev) { %>
    <a href="<%= pagination.prev %>" class="prev-page glyphicon glyphicon-circle-arrow-left">上一页</a>
  <% } %>
  <% if (pagination.next) { %>
    <a href="<%= pagination.next %>" class="next-page 	glyphicon glyphicon-circle-arrow-right">下一页</a>
  <% } %>
</div>
